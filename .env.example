# Gemini API Key
# Google AI Studio (https://aistudio.google.com/) で取得できます
GEMINI_API_KEY=your-gemini-api-key-here

# Firebase（クライアント用 - NEXT_PUBLIC_プレフィックスが必要）
NEXT_PUBLIC_FIREBASE_API_KEY=
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=
NEXT_PUBLIC_FIREBASE_PROJECT_ID=

# Firebase（サーバー用）
FIREBASE_PROJECT_ID=
FIREBASE_DATABASE_ID=komanavi

# Auth.js
# AUTH_SECRET は `npx auth secret` で生成できます
AUTH_SECRET=
AUTH_URL=http://localhost:3000

# Google OAuth
AUTH_GOOGLE_ID=
AUTH_GOOGLE_SECRET=

# Cloud Storage（漫画画像保存用）
GCS_MANGA_BUCKET=komanavi-manga-images
GCS_SIGNED_URL_TTL_MINUTES=60

# Cloud Tasks（漫画生成ジョブキュー）
CLOUD_TASKS_QUEUE=manga-generation
CLOUD_TASKS_LOCATION=asia-northeast1

MANGA_WORKER_URL=https://komanavi-worker-xxx.asia-northeast1.run.app/process
